{
  "name": "Elasticvue",
  "version": "0.19.0",
  "description": "Elasticsearch frontend",
  "manifest_version": 2,
  "icons": {
    "16": "./images/logo/manifest/blue_16.png",
    "32": "./images/logo/manifest/blue_32.png",
    "48": "./images/logo/manifest/blue_48.png",
    "128": "./images/logo/manifest/blue_128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {},
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "content_scripts":[{
      "all_frames": false,
      "matches":["*://*/"],
      "js":["background.js"],
      "run_at": "document_end"
  }]
}
